<p id="notice"><%= notice %></p>

<h1>Phishes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phishes.each do |phish| %>
      <tr>
        <td><%= link_to 'Show', phish %></td>
        <td><%= link_to 'Edit', edit_phish_path(phish) %></td>
        <td><%= link_to 'Destroy', phish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phish', new_phish_path %>
